    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^2.5.13"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0",
    "graphql-tag": "^2.12.6"
  },
  "peerDependencies": {
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0"
