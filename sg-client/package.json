{
  "name": "peerlocal/subgraph-client",
  "version": "1.0.6",
  "description": "",
  "main": "dist/peerlocal-subgraph-client.cjs.js",
  "module": "dist/peerlocal-subgraph-client.esm.js",
  "scripts": {
    "postinstall": "preconstruct dev",
    "codegen": "graphql-codegen --config src/graphql/codegen.ts",
